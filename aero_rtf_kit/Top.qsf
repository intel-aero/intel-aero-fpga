set_global_assignment -name PARTITION_NETLIST_TYPE SOURCE -section_id Top
set_global_assignment -name PARTITION_FITTER_PRESERVATION_LEVEL PLACEMENT_AND_ROUTING -section_id Top
set_global_assignment -name PARTITION_COLOR 16764057 -section_id Top

## 25MHz clock
set_location_assignment PIN_J5 -to in_CLK

## AEROFC <-> UART ESC
set_location_assignment PIN_R2 -to IO_MOTORS_Tx
set_location_assignment PIN_P1 -to IO_MOTORS_Rx
set_location_assignment PIN_L8 -to FC1_MOTORS_SCL_Tx
set_location_assignment PIN_R9 -to FC1_MOTORS_SDA_Rx

## AEROFC <-> CHT UART and Bootloader
set_location_assignment PIN_R7 -to FC1_XBEE_Rx
set_location_assignment PIN_P8 -to FC1_XBEE_Tx
set_location_assignment PIN_B12 -to CHT_DBG_UART_Tx
set_location_assignment PIN_B11 -to CHT_DBG_UART_Rx

## AEROFC <-> UART GPS
set_location_assignment PIN_A5 -to IO_GPS_Rx
set_location_assignment PIN_C8 -to IO_GPS_Tx
set_location_assignment PIN_R13 -to FC1_GPS_Rx
set_location_assignment PIN_K2 -to FC1_GPS_Tx

## AEROFC <-> DSM RC
set_location_assignment PIN_E11 -to IO_REC_Rx
set_location_assignment PIN_E9 -to IO_REC_Tx
set_location_assignment PIN_P6 -to FC1_IO3_REC_Rx
set_location_assignment PIN_M7 -to FC1_XBEE_CTS_REC_Tx

## AEROFC <-> TELEMETRY
set_location_assignment PIN_L5 -to IO_TELEM_Tx
set_location_assignment PIN_L4 -to IO_TELEM_Rx
set_location_assignment PIN_M5 -to FC1_TELEM_Tx
set_location_assignment PIN_P3 -to FC1_TELEM_Rx

## Booloader force pin
set_location_assignment PIN_R5 -to BOOTLOADER_FORCE_PIN

## Shared I2C
set_location_assignment PIN_L7 -to FC1_I2C_CLK
set_location_assignment PIN_L6 -to FC1_I2C_SDA
set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to FC1_I2C_SDA
set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to FC1_I2C_CLK

# External compass I2C
set_location_assignment PIN_P9 -to FC1_COMPASS_CLK
set_location_assignment PIN_P7 -to FC1_COMPASS_SDA
set_location_assignment PIN_D12 -to IO_COMPASS_CLK
set_location_assignment PIN_A14 -to IO_COMPASS_SDA
set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to FC1_COMPASS_CLK
set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to FC1_COMPASS_SDA
set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to IO_COMPASS_SDA

## Telemetry I2C
set_location_assignment PIN_N1 -to IO_TELEM_I2C_CLK
set_location_assignment PIN_P2 -to IO_TELEM_I2C_SDA
set_instance_assignment -name WEAK_PULL_UP_RESISTOR ON -to IO_TELEM_I2C_SDA

