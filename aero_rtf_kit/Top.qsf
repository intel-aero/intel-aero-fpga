set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to IO_REC_Tx
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to IO_TELEM_Rx
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to IO_TELEM_Tx
